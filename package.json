{
  "name": "sp-main",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "watchify": "watchify -vd -e src/Pages/Setting/main.js -o public/Modules/Setting/build.js",
    "dev": "npm-run-all --parallel watchify start",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o public/Modules/Setting/build.css ] -e src/Pages/Setting/main.js | uglifyjs -c warnings=false -m > public/Modules/Setting/build.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.15.2",
    "connect-flash": "^0.1.1",
    "connect-history-api-fallback": "^1.3.0",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.2.0",
    "envify": "^4.0.0",
    "express": "~4.14.0",
    "express-session": "^1.15.0",
    "express-validator": "^3.1.2",
    "html5-to-pdf": "^2.2.4",
    "jade": "~1.11.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.8.0",
    "morgan": "~1.7.0",
    "multer": "^1.3.0",
    "mysql": "^2.13.0",
    "nodemailer": "^3.0.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.8.3",
    "serve-favicon": "~2.3.0",
    "vee-validate": "^2.0.0-beta.25",
    "vue": "^2.1.10",
    "vue-resource": "^1.3.1",
    "vue-router": "^2.2.1",
    "vue-sortable": "^0.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-hmr": "^0.3.1",
    "browserify-middleware": "^7.1.0",
    "cross-env": "^4.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^2.3.0",
    "uglify-js": "^2.5.0",
    "vueify": "^9.4.0",
    "watchify": "^3.4.0"
  }
}
